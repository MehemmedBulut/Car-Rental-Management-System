@model List<Rent>
<div class="container mt-5">
    <div class="row">
        <div class="col-lg-12">
            <!-- Monthly Earnings -->
            <div class="card">
                <div class="card-body">
                    <div class="row align-items-center">
                        <div class="col-8">
                            <h5 class="card-title fw-bold mb-3">Aktiv ümumi gəlir</h5>
                            <h4 class="fw-bold mb-3">@ViewBag.AylıkToplamKiraBedeli$</h4>
                        </div>
                        <div class="col-4 text-end">
                            <div class="text-white bg-secondary rounded-circle p-3 d-flex align-items-center justify-content-center">
                                <i class="ti ti-currency-dollar fs-5"></i>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="earning" style="min-height: 60px;">
                    <div id="apexchartssparkline3" class="apexcharts-canvas apexchartssparkline3 apexcharts-theme-light" style="width: 326px; height: 60px;"></div>
                </div>
            </div>
        </div>
    </div>
</div>
