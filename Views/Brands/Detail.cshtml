<div class="container mt-5">
    <div class="row">
        <div class="col-md-6 offset-md-3">
            <div class="text-center">
                <h1>@Model.Name</h1>
            </div>
            <ul class="list-group mt-4">
                @foreach (Ctype item in Model.Types)
                {
                    <li class="list-group-item border border-2 border-dark text-center">@item.Name</li>
                }
            </ul>
            <div class="text-center mt-4">
                <a asp-action="Index" class="btn btn-primary">Geri</a>
            </div>
        </div>
    </div>
</div>
